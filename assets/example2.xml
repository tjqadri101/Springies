<model>
    <name>Example</name>
    <author>Robert Duvall</author>

    <nodes>
        <mass id="m0" x="100" y="100" vx="0" vy="0" mass="10.0"/>
        <mass id="m1" x="300" y="100" vx="0" vy="0" mass="10.0"/>
        <mass id="m2" x="200" y="300" vx="0" vy="0" mass="10.0"/>
        <mass id="m3" x="400" y="300" vx="0" vy="0" mass="10.0"/>
        <mass id="m4" x="500" y="500" vx="0" vy="0" mass="10.0"/>
    </nodes>

    <links>
        <spring a="m0" b="m1" restlength="150.0" constant="5"/>
        <muscle a="m2" b="m3" restlength="150" amplitude="20"/>
        <muscle a="m2" b="m4" restlength="150" amplitude="20"/>
        <muscle a="m3" b="m4" restlength="150" amplitude="20"/>
    </links>
</model>
